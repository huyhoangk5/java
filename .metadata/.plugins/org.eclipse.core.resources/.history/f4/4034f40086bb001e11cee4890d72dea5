package baitap;

public class CongNhan extends CanBo {
	private int rank;

	public CongNhan(int rank) {
		super();
		this.rank = rank;
	}
	

	public CongNhan(String name, int age, String gender, String address) {
		super(name, age, gender, address);
		// TODO Auto-generated constructor stub
	}


	public CongNhan() {
		super();
	}

	public int getRank() {
		return rank;
	}

	public void setRank(int rank) {
		this.rank = rank;
	}
	

}
